    "import re\n",
    "\n",
    "\n",
    "\n",
    "email = input(\"Enter your Email:\")\n",
    "pattern = r'^[a-zA-Z0-9._%+-]+@[a-zA-Z0-9.-]+[a-zA-Z]$'\n",
    "matchobj = re.match(pattern, email)\n",
    "\n",
    "if matchobj:\n",
    "    print(\"Email is valid\")\n",
    "else :\n",
    "    print(\"Email is not valid\")"
   ]
