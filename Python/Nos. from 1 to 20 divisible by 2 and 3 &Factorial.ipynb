   "source": [
    "## Factorial of a number\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 32,
   "id": "f907671c",
   "metadata": {},
   "outputs": [],
   "source": [
    "def fact(n):\n",
    "    if (n==1):\n",
    "        return 1\n",
    "    else:\n",
    "        return n * fact(n-1)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 35,
   "id": "70cc1d0e",
   "metadata": {},
   "outputs": [
